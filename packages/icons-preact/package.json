{
  "name": "@tabler/icons-preact",
  "version": "3.0.0",
  "license": "MIT",
  "author": "codecalm",
  "description": "A set of free MIT-licensed high-quality SVG icons for you to use in your web projects.",
  "homepage": "https://tabler-icons.io",
  "bugs": {
    "url": "https://github.com/tabler/tabler-icons/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/codecalm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabler/tabler-icons.git",
    "directory": "packages/icons-preact"
  },
  "type": "module",
  "main": "./dist/cjs/tabler-icons-preact.cjs",
  "types": "./dist/cjs/tabler-icons-preact.d.cts",
  "module": "./dist/esm/tabler-icons-preact.mjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/tabler-icons-preact.d.cts",
        "default": "./dist/cjs/tabler-icons-preact.cjs"
      },
      "import": {
        "types": "./dist/esm/tabler-icons-preact.d.ts",
        "default": "./dist/esm/tabler-icons-preact.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run clean && pnpm run copy:license && pnpm run build:icons && pnpm typecheck && pnpm run build:bundles",
    "build:icons": "node build.mjs",
    "build:bundles": "rollup -c ./rollup.config.mjs",
    "copy:license": "cp ../../LICENSE ./LICENSE",
    "clean": "rm -rf dist && find . ! -name '.gitkeep' -path '*/src/icons/*' -exec rm -rf {} +",
    "test": "vitest run",
    "typecheck": "tsc",
    "imports-check": "attw $(npm pack)"
  },
  "dependencies": {
    "@tabler/icons": "3.0.0"
  },
  "peerDependencies": {
    "preact": "^10.5.13"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.1",
    "@testing-library/preact": "^3.2.3",
    "preact": "^10.19.6"
  }
}
